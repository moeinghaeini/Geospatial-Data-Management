{
  "name": "geospatial-data-management",
  "version": "1.0.0",
  "description": "Modern geospatial data management application with interactive mapping and analysis",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev-complete": "concurrently \"npm run server\" \"npm run client\" \"npm run python\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "python": "cd python && python3 start_app.py",
    "python-setup": "cd python && python3 setup_python.py",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "install-python": "cd python && pip3 install -r requirements.txt",
    "start": "node server/index.js",
    "start-complete": "./start_complete_app.sh",
    "test": "npm run test:server && npm run test:client",
    "test:server": "jest server/**/*.test.js",
    "test:client": "cd client && npm test",
    "test:python": "cd python && python3 -m pytest tests/",
    "lint": "eslint server/**/*.js client/src/**/*.js",
    "lint:fix": "eslint server/**/*.js client/src/**/*.js --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "@turf/turf": "^6.5.0",
    "geojson": "^0.5.0",
    "multer": "^1.4.5-lts.1",
    "csv-parser": "^3.0.0",
    "axios": "^1.6.0",
    "ws": "^8.14.2",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "geospatial",
    "mapping",
    "gis",
    "postgis",
    "react",
    "nodejs"
  ],
  "author": "Geospatial Data Management Team",
  "license": "MIT"
}
